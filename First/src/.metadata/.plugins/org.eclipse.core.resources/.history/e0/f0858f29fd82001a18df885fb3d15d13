
public class hangManGame {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int guesses = 11;
		for (int i = guesses; i> 0; i--) {
			System.out.println( i + "heey");
		}

	}
	public static char[] theWord() {
		char[] randomWord = new char[6];
		char[] thePlayer = new char[randomWord.length];
		return thePlayer;
	}
	

}
